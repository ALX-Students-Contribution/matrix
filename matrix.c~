#include "matrix.h"

int main(int argc, char *argv[])
{
	int size = atoi(argv[1]);
	int i, j;

	if (argc != 2)
	{
		printf("usage: ./matrix <size>\n");
		return 1;
	}

	printf("Enter a %d x %d matrix. In this format: ", size, size);

	for (i = 0; i < size; i++)
	{
		for(j = 0; j < size; j++)
		{
			printf(" %d", rand() % 10);
		}
		printf(",");
	}
	printf("\n");

	

	return (0);
}
